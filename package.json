{
  "name": "source_commit",
  "version": "2.0.0",
  "description": "source_commit : Tool to display the sha.",
  "main": "./lib/index.js",
  "bin": {
    "source_commit": "./bin/source_commit.js"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "standard",
    "commit": "cz",
    "semantic-release": "semantic-release --branches main"
