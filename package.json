    "sha",
    "terminal",
    "translated-content"
  ],
  "author": "Juan Vasquez <me@juanvasquez.dev>",
  "license": "MIT",
  "dependencies": {
    "shelljs": "^0.8.5"
  },
  "devDependencies": {
    "cz-conventional-changelog": "^3.3.0",
    "semantic-release": "^19.0.3",
    "standard": "^17.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JuanVqz/source_commit.git"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
